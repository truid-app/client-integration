{
  "name": "@veritru/client-integration",
  "version": "0.1.0",
  "description": "Client integration documentation and examples",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "mkdir -p ./build && yarn run build:puml",
    "build:puml": "for f in $(find docs -name '*.puml') ; do mkdir -p build/$(dirname $f) && puml generate --svg -o build/${f%.puml}.svg $f ; done",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-plantuml": "^0.9.0"
  }
}
